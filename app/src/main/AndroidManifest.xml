<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teddy.praktekdokterivonne">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherxhdi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcherxhdi"
        android:supportsRtl="true"
        android:theme="@style/Theme.PraktekDokterIvonne">

        <activity android:name=".PartnershipActivity" />
        <activity android:name=".AplikasiActivity" />
        <activity android:name=".ChatbotActivity" />
        <activity android:name=".KontakActivity" />
        <activity android:name=".DokumentasiActivity" />
        <activity android:name=".ArtikelActivity" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".LayananActivity" />
        <activity android:name=".TentangDokterActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />

    </application>

</manifest>
